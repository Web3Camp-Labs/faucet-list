    <script type='module' src="js/chain.js?r=1" defer></script>

    <script src="js/common.js"></script>
    <script type="text/javascript">
    $(document).ready(function() {
        const swiper = new Swiper('.tmy-news-slider', {
            slidesPerView: 1,
            centeredSlides: true,
            loop: true,
            navigation: {
                prevEl: '.cstm-control-btn-prev',
                nextEl: '.cstm-control-btn-next'
            },
            breakpoints: {
                0: {
                    slidesPerView: 1.2,

                },
                576: {
                    slidesPerView: 2,
                },
                992: {
                    slidesPerView: 3,

                }
            }
        });


        $('.responsive_menu .dropdown-element > a').click(function(){
            $(this).next().slideToggle();
        });

        $('.rp-menu-btn,.closeBtn').click(function(){
            $('.responsive_menu').toggleClass('--opened');
        });
      

        const breakpoint = window.matchMedia('(min-width:991px)');
        let mySwiper;
        const breakpointChecker = function() {
            if (breakpoint.matches === true) {
                if (mySwiper !== undefined) mySwiper.destroy(true, true);
                return;
            } else if (breakpoint.matches === false) {
                return enableSwiper();
            }
        };

        const enableSwiper = function() {
            mySwiper = new Swiper('.tmy-stat-boxes', {
                loop: true,
                slidesPerView: 1.5,
                spaceBetween: 15,
                centeredSlides: true,
                grabCursor: true,
                paginationClickable: true,
                autoplay: {
                    delay: 2000,
                    disableOnInteraction: false,
                },
                breakpoints: {
                    0: {
                        slidesPerView: 1.2,
                    },
                    576: {
                        slidesPerView: 2,
                    },
                    767: {
                        slidesPerView: 3,

                    }
                }
            });

        };

        breakpoint.addListener(breakpointChecker);
        breakpointChecker();



        $('.faq-tab-item-heading').click(function() {
            $('.faq-tab-item-active').removeClass('faq-tab-item-active');
            $(".faq-tab-item-content").slideUp('fast');
            $(this).next().slideToggle('fast');
            $(this).parent().toggleClass('faq-tab-item-active');
        });

        $('.scrollUp[href*="#"]').on('click', function(e) {
            e.preventDefault();
            $('html, body').animate({
                scrollTop: $($(this).attr('href')).offset().top
            }, 500, 'linear');
        });

        $(window).scroll(function() {
            if ($(this).scrollTop() > 450) {
                $('.scrollUp').css('visibility', 'visible');
            } else {
                $('.scrollUp').css('visibility', 'hidden');
            }
        });



    });
    </script>
</body>

